{
	"name": "modmail",
	"description": "✉️",
	"packageManager": "yarn@3.2.1",
	"main": "./dist/index.js",
	"version": "0.1.0-dev",
	"type": "module",
	"author": {
		"name": "DD",
		"email": "didinele.dev@gmail.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/chatsift/modmail.git"
	},
	"bugs": {
		"url": "https://github.com/chatsift/modmail/issues"
	},
	"homepage": "https://github.com/chatsift/modmail",
	"imports": {
		"#struct/*": "./dist/struct/*.js",
		"#util/*": "./dist/util/*.js"
	},
	"scripts": {
		"lint": "eslint src --ext .ts && prettier --check --plugin-search-dir=. .",
		"build": "tsc",
		"format": "prettier --write --plugin-search-dir=. .",
		"test": "jest",
		"test:ci": "jest --ci --no-stack-trace --verbose",
		"test:debug": "jest --runInBand",
		"prisma": "dotenv -e .env prisma",
		"deploy-commands": "yarn build && dotenv -e .env -v DEPLOY=true -- node --es-module-specifier-resolution=node --enable-source-maps ./dist/index.js",
		"start": "dotenv -e .env -- node --es-module-specifier-resolution=node --enable-source-maps ./dist/index.js",
		"prepare": "is-ci || husky install",
		"update": "yarn upgrade-interactive"
	},
	"engines": {
		"node": ">=16.9.0"
	},
	"devDependencies": {
		"@babel/core": "^7.18.6",
		"@babel/generator": "^7.18.7",
		"@babel/plugin-proposal-decorators": "^7.18.6",
		"@babel/preset-env": "^7.18.6",
		"@babel/preset-typescript": "^7.18.6",
		"@commitlint/cli": "^17.0.3",
		"@commitlint/config-angular": "^17.0.3",
		"@types/common-tags": "^1.8.1",
		"@types/i18next-fs-backend": "^1.1.2",
		"@types/jest": "^27.5.2",
		"@types/node": "^16.11.42",
		"@typescript-eslint/eslint-plugin": "^5.30.3",
		"@typescript-eslint/parser": "^5.30.3",
		"babel-jest": "^28.1.2",
		"babel-plugin-const-enum": "^1.2.0",
		"babel-plugin-transform-typescript-metadata": "^0.3.2",
		"dotenv-cli": "^5.1.0",
		"eslint": "^8.19.0",
		"eslint-config-marine": "^9.4.1",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.26.0",
		"husky": "^8.0.1",
		"jest": "^28.1.2",
		"prettier": "^2.7.1",
		"prettier-eslint": "^15.0.1",
		"prisma": "^4.0.0",
		"typescript": "^4.7.4"
	},
	"dependencies": {
		"@chatsift/readdir": "^0.1.0",
		"@discordjs/rest": "^0.5.0",
		"@naval-base/ms": "^3.1.0",
		"@prisma/client": "^4.0.0",
		"bree": "^9.1.0",
		"common-tags": "^1.8.2",
		"discord.js": "dev",
		"i18next": "^21.8.11",
		"i18next-fs-backend": "^1.1.4",
		"nanoid": "^4.0.0",
		"prisma-error-enum": "^0.1.2",
		"reflect-metadata": "^0.1.13",
		"tslib": "^2.4.0",
		"tsyringe": "^4.7.0"
	}
}
